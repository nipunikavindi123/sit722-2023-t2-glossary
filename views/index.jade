doctype html
html
  head
    title= title
    link(rel='stylesheet', href='https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/css/bootstrap.min.css')
  body
    div.container
      h1= title
      h5= shortdescription
      table.table.table-striped
        thead
          tr
            th #ID
            th Term
            th Description
            th References
        tbody
          each data in tableData
            tr
              td= data.id 
              td= data.term
              td= data.description
              td= data.references
